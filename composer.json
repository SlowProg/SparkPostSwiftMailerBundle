{
    "name": "slowprog/sparkpost-swiftmailer-bundle",
    "description": "Symfony bundle to provide a SparkPost SwiftMailer service",
    "keywords": ["sparkpost", "swiftmailer", "symfony", "bundle"],
    "homepage": "https://github.com/SlowProg/SparkPostSwiftMailerBundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Andrey Tyshev",
            "email": "slowprog@gmail.com"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url" : "https://github.com/SlowProg/SparkPostSwiftMailer.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": ">=2.3",
        "symfony/swiftmailer-bundle": ">=2.3",
        "slowprog/sparkpost-swiftmailer": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "swiftmailer/swiftmailer": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "symfony/finder": ">=2.3",
        "symfony/config": ">=2.3"
    },
    "autoload": {
        "psr-0": { "SlowProg\\SparkPostSwiftMailerBundle": "" }
    },
    "target-dir": "SlowProg/SparkPostSwiftMailerBundle"
}